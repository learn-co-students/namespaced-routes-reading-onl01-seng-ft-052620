exit
@last_post.created_at.strftime("%d, %, %y")
@last_post.created_at.strftime("%b, %d, %y")
@last_post.created_at.strftime("%b, %e, %C")
@last_post.created_at.strftime("%b, %d, %C")
@last_post.created_at.strftime("%m, %d, %C")
@last_post.created_at.strftime("%Y, %d")
@last_post.created_at.strftime(%Y, %d)
@last_post.created_at(%d)
@last_post.date
@last_post
